{
  "name": "lazy-provide-inject",
  "version": "2.3.0",
  "type": "module",
  "description": "A utility library to simplify and enhance the usage of Vue 3's Composition API `provide` and `inject` features with type safety.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aose-yuu/lazy-provide-inject.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "provide",
    "inject",
    "lazy"
  ],
  "author": "aose-yuu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aose-yuu/lazy-provide-inject/issues"
  },
  "homepage": "https://github.com/aose-yuu/lazy-provide-inject#readme",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "npm run build"
  },
  "types": "./types/input.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "browser": "./dist/lazy-provide-inject.umd.js",
  "module": "./dist/lazy-provide-inject.es.js",
  "exports": {
    ".": {
      "import": "./dist/lazy-provide-inject.es.js",
      "require": "./dist/lazy-provide-inject.umd.js",
      "types": "./types/input.d.ts"
    }
  },
  "dependencies": {
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
